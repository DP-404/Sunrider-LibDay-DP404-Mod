    fon "(Quizás los viejos días de la Revolución Compact puedan regresar...)"

# game/script.rpy:9454
translate spanish commanderunfairme_f2d18de7:

    # fon "(When we traveled the stars to liberate ourselves from the New Empire...)"
    fon "(Cuando viajábamos por las estrellas para liberarnos del Nuevo Imperio...)"

# game/script.rpy:9455
translate spanish commanderunfairme_b9b25fca:

    # fon "(Alice is now dead.)"
    fon "(Alice está ahora muerta.)"

# game/script.rpy:9456
translate spanish commanderunfairme_ab987214:

