    # sol "Understood..."
    sol "Entendido..."

# game/REturn.rpy:7190
translate spanish dontpulltrigger_e3c6fb1c:

    # sol "Whichever path you take... I shall be beside you."
    sol "Cualquiera que sea el camino que tomes... yo estaré a tu lado."

# game/REturn.rpy:7191
