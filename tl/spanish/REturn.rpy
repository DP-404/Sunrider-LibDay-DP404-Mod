    # asa "Huh?"
    asa "¿Huh?"

# game/REturn.rpy:7220
translate spanish dontpulltrigger_d1d3fe5e:

    # kay "You'll know when the time comes."
    kay "Lo sabrás cuando la hora llegue."

# game/REturn.rpy:7222
translate spanish dontpulltrigger_316c63f6_1:

