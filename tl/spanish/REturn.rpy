    asa "... ... ..."

# game/REturn.rpy:7209
translate spanish dontpulltrigger_8b0e0386:

    # asa "If you're sure 'bout this... then I guess I can go along with it... but..."
    asa "Si estás seguro acerca de esto... entonces supongo que puedo estar de acuerdo con ello... Pero..."

# game/REturn.rpy:7210
translate spanish dontpulltrigger_055059aa:

    # kay "The future is not for us to change."
    kay "No debemos cambiar el futuro."

# game/REturn.rpy:7211
translate spanish dontpulltrigger_1d77e754:

    # kay "We would never be heroes if we murdered Chigara today."
    kay "Nosotros nunca seríamos héroes si asesinásemos a Chigara hoy."

# game/REturn.rpy:7213
translate spanish dontpulltrigger_b6809cdc:

    # "Asaga reluctantly smiled."
    "Asaga sonrió con disgusto."

# game/REturn.rpy:7214
translate spanish dontpulltrigger_08cb6e7a:

    # asa "All right..."
    asa "Está bien..."

# game/REturn.rpy:7215
translate spanish dontpulltrigger_5c169b3e_1:

    # "He smiled and took her hand."
    "Él sonrió y tomó su mano."

# game/REturn.rpy:7216
translate spanish dontpulltrigger_e770f05a:

    # kay "This isn't the end."
    kay "Este no es el fin."

# game/REturn.rpy:7217
translate spanish dontpulltrigger_42e607d1:

    # kay "I'm counting on you... to rescue me again."
    kay "Cuento contigo... para que me rescates otra vez."

# game/REturn.rpy:7219
